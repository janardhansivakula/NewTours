package com.IfElseDemo;

public class IfElseProg_SmallestThree_Num 
{
	
	
	public static void main(String[]args)
	{
		 int big=100;
		
		int small1=11;
		if(small1<big)
		{
			System.out.println("The Small Num1:"+small1);
		}
		else
		{
			System.out.println("It's Not a Smallest Number Num1"); 
			
		}
		
		int small2=12;
		
		if(small2<big)
		{
			System.out.println("The Small Num2:"+small2);
		}
		else
		{
			System.out.println("It's Not a Smallest Number Than Num3"); 
			
		}
		int small3=13;
		if(small3<big)
		{
			System.out.println("The Small Num3:"+small3);
		}
		else
		{
			System.out.println("It's Not a Smallest Number Than First Small Num"); 
		}
	}
}
