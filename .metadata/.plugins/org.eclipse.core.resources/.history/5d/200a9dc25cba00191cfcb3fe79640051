package com.IfElseDemo;

public class IfElseProg_SmallestThree_Num 
{
	public static void main(String[]args)
	{
		int a=10;
		int b=20;
		int c=30;
		int d=40;
		int e=50;
		int f=60;
		if(a<b)
		{
			System.out.println(a);
		}
		else if(b<c)
		{
			System.out.println(b);
		}
		else if(c<d)
		{
			System.out.println(c);
		}
		else if(d<e)
		{
			System.out.println(d);
		}
		else if(e<f)
		{
			System.out.println(e);
		}
	}
}
